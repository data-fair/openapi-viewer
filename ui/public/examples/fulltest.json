    "/Request": {
      "get": {
        "summary": "Obtenir des informations sur les requêtes",
        "description": "Cette route permet de tester différents paramètres de requête.",
        "operationId": "getRequest",
        "tags": [
          "A Tag"
        ],
        "parameters": [
          {
            "name": "testString",
            "in": "query",
            "description": "Un paramètre de chaîne de caractères pour tester",
            "schema": {
              "title": "Une string",
              "type": "string"
            }
          },
          {
            "name": "requiredString",
            "in": "query",
            "description": "Un paramètre de chaîne de caractères requis",
            "required": true,
            "schema": {
              "title": "Une string requise",
              "type": "string"
            }
          },
          {
            "name": "deprecatedString",
            "in": "query",
            "description": "Un paramètre de chaîne de caractères déprécié",
            "deprecated": true,
            "schema": {
              "title": "Une string dépréciée",
              "type": "string"
            }
          },
          {
            "name": "testBoolean",
            "in": "query",
            "description": "Un paramètre booléen pour tester",
            "schema": {
              "title": "Un booléen",
              "type": "boolean"
            }
          },
          {
            "name": "testInteger",
            "in": "query",
            "description": "Un paramètre entier pour tester, compris entre **1** et **10**, avec une valeur par défaut de **5**",
            "schema": {
              "type": "integer",
              "minimum": 1,
              "maximum": 10,
              "default": 5
            }
          },
          {
            "name": "testEnum",
            "in": "query",
            "description": "Un paramètre enum pour tester",
            "schema": {
              "title": "Une enum",
              "type": "string",
              "enum": ["value1", "value2", "value3"]
            }
          },
          {
            "name": "testObject",
            "in": "query",
            "description": "Tous les paramètres de cet objet doivent commencer par un préfixe :\n\n  - `eq_`\n  - `lg_`.\n\nIls seront transformés en simples paramètres de query lors de la requête (ex: `?eq_param1=value1&lg_param2=value2`).",
            "schema": {
              "title": "Un sous-objet",
              "type": "object",
              "patternProperties": {
                "eq_(.*)": {
                  "type": "string"
                },
                "lg_(.*)": {
                  "type": "number"
                }
              }
            }
          },
          {
            "name": "testArrayOneOf",
            "in": "query",
            "description": "Un paramètre tableau de chaînes de caractères avec oneOf pour tester",
            "schema": {
              "title": "Un tableau de oneOf",
              "type": "array",
              "items": {
                "type": "string",
                "oneOf": [
                  {
                    "const": "value1",
                    "title": "Valeur 1"
                  },
                  {
                    "const": "value2",
                    "title": "Valeur 2"
                  },
                  {
                    "const": "value3",
                    "title": "Valeur 3"
                  }
                ]
              }
            }
          },
          {
            "name": "example1",
            "in": "query",
            "description": "Un parametre avec un simple exemple",
            "schema": {
              "title": "Une string",
              "type": "string"
            },
            "example": "Je suis l'exemple"
          },
          {
            "name": "example2",
            "in": "query",
            "description": "Un parametre avec des exemples",
            "schema": {
              "title": "Une string",
              "type": "string"
            },
            "examples": {
              "example1": {
                "summary": "Exemple 1",
                "value": "Je suis l'exemple 1"
              },
              "example2": {
                "summary": "Exemple 2",
                "value": "Je suis l'exemple 2"
              }
            }
          },
          {
            "name": "example3",
            "in": "query",
            "description": "Un parametre avec un exemple mais du mauvais type",
            "schema": {
              "title": "Une string",
              "type": "string"
            },
            "example": {
              "title": "Je suis un objet exemple"
            }
          },
          {
            "name": "example4",
            "in": "query",
            "description": "Un parametre avec un exemple dans le schema",
            "schema": {
              "title": "Une string",
              "type": "string",
              "examples": [
                "Je suis l'exemple"
              ]
            }
          },
          {
            "name": "example5",
            "in": "query",
            "description": "Un parametre avec un exemple dans le schema et un exemple dans le parametre",
            "schema": {
              "title": "Une string",
              "type": "string",
              "examples": [
                "Je suis l'exemple"
              ]
            },
            "example": "Je suis l'exemple 2"
          }
        ],
        "responses": {
          "2XX": {
            "description": "Toues les réponses commençant par 2 sont des réussites",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GeneralResponse"
                }
              }
            }
          }
        }
      }
    },
